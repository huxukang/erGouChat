{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/tab/chat/chat",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/tab/friends/friends",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/tab/find/find",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/tab/mine/mine",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	"tabBar": {
		"fontSize": "12px", //字体
		"borderStyle": "black", //上边框颜色（仅支持白和黑）
		"color": "#000000", //默认时底部字体颜色
		"selectedColor": "#1ba824", //选中时的文字颜色
		"backgroundColor": "rgba(250,250,250)", //背景颜色
		"position": "bottom", //可选top，bottom（top仅支持微信小程序）
		"list": [ //底部以数组形式添加，2--5包括2,5
			{
				"text": "聊天室", //底部文字信息
				"pagePath": "pages/tab/chat/chat", //页面路径
				"iconPath": "static/tarbar/chat.png", //默认时
				"selectedIconPath": "static/tarbar/chatActived.png" //点击时

			},
			{
				"text": "好友", //底部文字信息
				"pagePath": "pages/tab/friends/friends", //页面路径
				"iconPath": "static/tarbar/chat.png", //默认时
				"selectedIconPath": "static/tarbar/chatActived.png" //点击时

			},
			{
				"text": "发现", //底部文字信息
				"pagePath": "pages/tab/find/find", //页面路径
				"iconPath": "static/tarbar/chat.png", //默认时
				"selectedIconPath": "static/tarbar/chatActived.png" //点击时

			},
			{
				"text": "我", //底部文字信息
				"pagePath": "pages/tab/mine/mine", //页面路径
				"iconPath": "static/tarbar/chat.png", //默认时
				"selectedIconPath": "static/tarbar/chatActived.png" //点击时
			
			}
		]
	}
}
